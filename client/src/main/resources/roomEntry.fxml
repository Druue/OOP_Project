<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="roomEntryBackground" prefHeight="200.0" prefWidth="600.0" style="-fx-background-color: #C0C0C0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox>
         <children>
            <Label fx:id="roomName" text="Room 1">
               <font>
                  <Font size="50.0" />
               </font>
               <padding>
                  <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
               </padding></Label>
            <Region HBox.hgrow="ALWAYS" />
            <VBox prefHeight="200.0" prefWidth="100.0" spacing="10.0">
               <children>
                  <Label fx:id="hasMultimedia" text="hasMultimedia">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin></Label>
                  <HBox prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label alignment="CENTER_RIGHT" text="Capacity:" />
                        <Label fx:id="capacity" text="x" />
                     </children>
                  </HBox>
               </children>
               <HBox.margin>
                  <Insets right="80.0" />
               </HBox.margin>
               <padding>
                  <Insets top="40.0" />
               </padding>
            </VBox>
         </children>
      </HBox>
      <HBox prefHeight="160.0" prefWidth="200.0">
         <children>
            <StackPane fx:id="timeline" alignment="TOP_LEFT" prefWidth="450.0" HBox.hgrow="NEVER">
               <HBox.margin>
                  <Insets bottom="20.0" left="20.0" top="10"/>
               </HBox.margin></StackPane>
            <Button fx:id="reserveButton" mnemonicParsing="false" style="-fx-background-color: red; -fx-text-fill: white;" text="Reserve">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <font>
                  <Font size="14.0" />
               </font>
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin></Button>
         </children>
      </HBox>
   </children>
</VBox>
