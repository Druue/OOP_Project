<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.Box?>
<?import javafx.scene.text.*?>
<VBox xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="640.0" stylesheets="@homepage.css"
      xmlns="http://javafx.com/javafx/11.0.1" fx:controller="nl.tudelft.oopp.client.controllers.AddBuildingsController">
    <children>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
            <children>
                <ImageView fitWidth="800" pickOnBounds="true" preserveRatio="true" AnchorPane.leftAnchor="0.0"
                           AnchorPane.rightAnchor="0.0">
                    <image>
                        <Image url="@images/Rooms-background8x6.png"/>
                    </image>
                </ImageView>
                <AnchorPane prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="80.0"
                            AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="100.0">
                    <children>
                        <Box depth="0.0" height="420.0" width="800.0" AnchorPane.bottomAnchor="0.0"
                             AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
                        <VBox prefHeight="420.0" prefWidth="429.0" AnchorPane.bottomAnchor="80.0"
                              AnchorPane.topAnchor="0.0">
                            <children>
                                <VBox prefHeight="114.0" prefWidth="463.0">
                                    <children>
                                        <Label text="Add a new Building">
                                            <font>
                                                <Font name="Helvetica" size="27.0"/>
                                            </font>
                                            <VBox.margin>
                                                <Insets left="25.0" top="17.0"/>
                                            </VBox.margin>
                                        </Label>
                                    </children>
                                </VBox>
                                <VBox prefHeight="109.0" prefWidth="429.0">
                                    <children>
                                        <VBox prefHeight="23.0" prefWidth="463.0">
                                            <children>
                                                <Label text="Buidling number">
                                                    <font>
                                                        <Font name="Helvetica" size="14.0"/>
                                                    </font>
                                                    <VBox.margin>
                                                        <Insets left="25.0"/>
                                                    </VBox.margin>
                                                </Label>
                                            </children>
                                        </VBox>
                                        <VBox prefHeight="36.0" prefWidth="463.0">
                                            <children>
                                                <TextField fx:id="buildingNumberInput" prefHeight="30.0"
                                                           prefWidth="404.0">
                                                    <VBox.margin>
                                                        <Insets left="25.0"/>
                                                    </VBox.margin>
                                                </TextField>
                                            </children>
                                        </VBox>
                                    </children>
                                </VBox>
                                <VBox prefHeight="101.0" prefWidth="429.0">
                                    <children>
                                        <VBox prefHeight="22.0" prefWidth="463.0">
                                            <children>
                                                <Label text="Name">
                                                    <font>
                                                        <Font name="Helvetica" size="14.0"/>
                                                    </font>
                                                    <VBox.margin>
                                                        <Insets left="25.0"/>
                                                    </VBox.margin>
                                                </Label>
                                            </children>
                                        </VBox>
                                        <VBox prefHeight="69.0" prefWidth="463.0">
                                            <children>
                                                <TextField fx:id="buildingNameInput" prefHeight="30.0"
                                                           prefWidth="400.0">
                                                    <VBox.margin>
                                                        <Insets left="25.0"/>
                                                    </VBox.margin>
                                                </TextField>
                                            </children>
                                        </VBox>
                                    </children>
                                </VBox>
                                <VBox prefHeight="178.0" prefWidth="463.0">
                                    <children>
                                        <VBox prefHeight="31.0" prefWidth="463.0">
                                            <children>
                                                <Label text="Details">
                                                    <font>
                                                        <Font name="Helvetica" size="14.0"/>
                                                    </font>
                                                    <VBox.margin>
                                                        <Insets left="25.0"/>
                                                    </VBox.margin>
                                                </Label>
                                            </children>
                                        </VBox>
                                        <VBox prefHeight="200.0" prefWidth="100.0">
                                            <children>
                                                <TextField fx:id="buildingDescriptionInput" prefHeight="108.0"
                                                           prefWidth="463.0">
                                                    <VBox.margin>
                                                        <Insets left="25.0"/>
                                                    </VBox.margin>
                                                </TextField>
                                            </children>
                                        </VBox>
                                    </children>
                                </VBox>
                            </children>
                        </VBox>
                        <VBox layoutX="489.0" layoutY="78.0" prefHeight="262.0" prefWidth="311.0">
                            <children>
                                <VBox prefHeight="148.0" prefWidth="311.0">
                                    <children>
                                        <VBox prefHeight="200.0" prefWidth="100.0">
                                            <children>
                                                <Label text="Opening time">
                                                    <font>
                                                        <Font name="Helvetica" size="14.0"/>
                                                    </font>
                                                </Label>
                                                <VBox prefHeight="44.0" prefWidth="311.0">
                                                    <children>
                                                        <TextField fx:id="buildingOpeningTimeInput" prefHeight="30.0"
                                                                   prefWidth="161.0">
                                                            <VBox.margin>
                                                                <Insets right="150.0"/>
                                                            </VBox.margin>
                                                        </TextField>
                                                    </children>
                                                </VBox>
                                            </children>
                                        </VBox>
                                    </children>
                                </VBox>
                                <VBox prefHeight="146.0" prefWidth="331.0">
                                    <children>
                                        <VBox prefHeight="15.0" prefWidth="311.0">
                                            <children>
                                                <Label text="Closing time">
                                                    <font>
                                                        <Font name="Helvetica" size="14.0"/>
                                                    </font>
                                                    <VBox.margin>
                                                        <Insets top="10.0"/>
                                                    </VBox.margin>
                                                </Label>
                                            </children>
                                        </VBox>
                                        <VBox prefHeight="200.0" prefWidth="100.0">
                                            <children>
                                                <TextField fx:id="buildingClosingTimeInput" prefHeight="30.0"
                                                           prefWidth="50.0">
                                                    <VBox.margin>
                                                        <Insets right="150.0"/>
                                                    </VBox.margin>
                                                </TextField>
                                            </children>
                                        </VBox>
                                    </children>
                                </VBox>
                                <VBox prefHeight="200.0" prefWidth="100.0">
                                    <children>
                                        <VBox prefHeight="112.0" prefWidth="331.0">
                                            <children>
                                                <Label text="Image">
                                                    <font>
                                                        <Font name="Helvetica" size="14.0"/>
                                                    </font>
                                                    <VBox.margin>
                                                        <Insets top="10.0"/>
                                                    </VBox.margin>
                                                </Label>
                                            </children>
                                        </VBox>
                                        <VBox prefHeight="200.0" prefWidth="100.0">
                                            <children>
                                                <TextField prefHeight="30.0" prefWidth="331.0">
                                                    <VBox.margin>
                                                        <Insets right="50.0" top="10.0"/>
                                                    </VBox.margin>
                                                </TextField>
                                            </children>
                                        </VBox>
                                    </children>
                                </VBox>
                            </children>
                        </VBox>
                        <Button onAction="#addBuilding" prefHeight="30.0" style="-fx-background-color: #287e40;"
                                text="Add" textFill="WHITE" AnchorPane.bottomAnchor="40.0" AnchorPane.leftAnchor="320.0"
                                AnchorPane.rightAnchor="320.0">
                            <font>
                                <Font name="Helvetica" size="18.0"/>
                            </font>
                        </Button>
                    </children>
                </AnchorPane>

                <MenuBar id="menuBar" style="-fx-background-color: #00A6D6;" AnchorPane.leftAnchor="0.0"
                         AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <menus>
                        <Menu mnemonicParsing="false" text="Reserve">
                            <items>
                                <MenuItem onAction="#goToRes" text="Room"/>
                                <MenuItem mnemonicParsing="false" text="Bike"/>
                            </items>
                        </Menu>
                        <Menu text="Manage buildings">
                            <MenuItem onAction="#goToAddBuildings" text="Add building"/>
                            <MenuItem mnemonicParsing="false" text="Edit buildings"/>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Manage reservables">
                            <items>
                                <MenuItem onAction="#goToAddRooms" text="Add a room"/>
                                <MenuItem mnemonicParsing="false" text="Add a bike"/>
                                <MenuItem text="Edit reservables"/>
                            </items>
                        </Menu>
                        <Menu id="inactiveMenu" mnemonicParsing="false"
                              text="                                                                     ">
                        </Menu>
                        <Menu mnemonicParsing="false" text="more..">
                            <items>
                                <MenuItem mnemonicParsing="false" text="Log out"/>
                                <MenuItem mnemonicParsing="false" text="Copyrights"/>
                            </items>
                        </Menu>
                    </menus>
                </MenuBar>
                <Button id="logo" onAction="#goToAdmin" prefWidth="100.0" AnchorPane.rightAnchor="80.0"
                        AnchorPane.topAnchor="0.0"/>
                <HBox layoutY="2.0" prefHeight="30.0" prefWidth="100.0" AnchorPane.rightAnchor="80.0"
                      AnchorPane.topAnchor="2.0">
                    <children>
                        <ImageView fitHeight="26.0" fitWidth="114.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@images/Logo.png"/>
                            </image>
                        </ImageView>
                    </children>
                </HBox>
            </children>
        </AnchorPane>
    </children>
</VBox>
